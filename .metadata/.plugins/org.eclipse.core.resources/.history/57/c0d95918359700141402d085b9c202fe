#ifndef CONTACTO_H_
#define CONTACTO_H_

using namespace std;

class Contacto {

private:

	string _nombre;
	string _apellidos;
	string _direccion;
	string _telefono;
	string _dni;


public:
	Contacto();
	virtual ~Contacto();
};

#endif /* CONTACTO_H_ */
