}

const std::string& Contacto::getAnotaciones() const {
	return _anotaciones;
}

void Contacto::setAnotaciones(const std::string& anotaciones) {
	_anotaciones = anotaciones;
}

const std::string& Contacto::getApellidos() const {
	return _apellidos;
}

void Contacto::setApellidos(const std::string& apellidos) {
	_apellidos = apellidos;
}

const direccion& Contacto::getDirecciones() const {
	return _direcciones;
}

void Contacto::setDirecciones(const direccion& direcciones) {
	_direcciones = direcciones;
}

const std::string& Contacto::getDni() const {
	return _dni;
}

void Contacto::setDni(const std::string& dni) {
	_dni = dni;
}

const emails& Contacto::getEmail() const {
	return _email;
}

void Contacto::setEmail(const emails& email) {
	_email = email;
}

bool Contacto::isFavorito() const {
	return _favorito;
}

void Contacto::setFavorito(bool favorito) {
	_favorito = favorito;
}

const std::string& Contacto::getNombre() const {
	return _nombre;
}

void Contacto::setNombre(const std::string& nombre) {
	_nombre = nombre;
}

const redes_sociales& Contacto::getRedes() const {
	return _redes;
}

void Contacto::setRedes(const redes_sociales& redes) {
	_redes = redes;
}

telefonos Contacto::getTelefono() const {
	return _telefono;
}

void Contacto::setTelefono(telefonos telefono) {
	_telefono = telefono;
}

